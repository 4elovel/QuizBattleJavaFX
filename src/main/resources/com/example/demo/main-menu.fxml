  spacing="15" stylesheets="@menus-styles.css" xmlns="http://javafx.com/javafx"
  fx:controller="com.example.demo.controllers.MainMenuController"
  fx:id="menuBox">

  <Button styleClass="menu-button" text="Start" fx:id="btnStart"/>
  <Button styleClass="menu-button" text="Einstellungen" fx:id="btnSettings"/>
  <Button styleClass="menu-button" text="Exit" fx:id="btnExit"/>
</VBox>
